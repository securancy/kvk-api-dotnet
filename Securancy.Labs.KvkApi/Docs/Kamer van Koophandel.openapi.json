{
    "openapi": "3.0.1",
    "info": {
        "title": "Kamer van Koophandel",
        "description": "Documentatie voor API Basisprofiel.",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "https://api.securancy.io/services/api/kvk"
        }
