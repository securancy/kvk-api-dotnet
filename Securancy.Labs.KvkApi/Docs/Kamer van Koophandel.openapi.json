                        }
                    },
                    {
                        "name": "geoData",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "boolean",
                            "default": false
                        }
                    }
                ],
                "responses": {
                    "default": {
                        "description": "default response",
                        "content": {
                            "application/hal+json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Eigenaar"
                                }
                            }
                        }
                    }
                }
            }
        },
